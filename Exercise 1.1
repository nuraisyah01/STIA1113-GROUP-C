package printPattern;

public class PrintTriangularPattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Print Triangular Pattern
		
		System.out.println ("* * * * *");
		System.out.println (" *     *");
		System.out.println ("  *   *");
